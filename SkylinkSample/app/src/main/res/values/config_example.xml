<?xml version="1.0" encoding="utf-8"?>

<!-->
  Instructions for using the config_example.xml file.
  To use your own App Key and secret for this app:
  MAKE A COPY of config_example.xml (do NOT edit config_example.xml for app usage)
    and give it a name not used in this directory (res/value/), e.g. config.xml.
  Uncomment the xml elements in it.
  Populate the boolean value for is_app_key_smr with whether the app should start by selecting
    app_key_no_smr (false) or app_key_smr (true) as the App key to use.
  Populate the values for app_key_no_smr and app_key_smr with the appropriate app keys.
  Populate the values for app_key_secret_no_smr and app_key_secret_smr with the appropriate app secret.
  Populate the values for app_key_desc_no_smr and app_key_desc_smr with the appropriate app secret.
<-->
<resources>

    <!-- App Keys and secrets. -->

    Uncomment in config.xml
    <bool name="is_app_key_smr">false</bool>

    Uncomment in config.xml
    <string name="app_key_no_smr">8df95c06-b31d-4771-976a-aef7ec0950aa</string>
    <string name="app_key_secret_no_smr">ag68syk4u6pta</string>
    <string name="app_key_desc_no_smr">tourguide</string>

    Uncomment in config.xml
    <string name="app_key_smr">8df95c06-b31d-4771-976a-aef7ec0950aa</string>
    <string name="app_key_secret_smr">ag68syk4u6pta</string>
    <string name="app_key_desc_smr">tourguide</string>

</resources>
